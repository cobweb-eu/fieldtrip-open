[install]

; name of the project
name=FieldTripExample

; the source of this file
; e.g <user>@<domain>:<path of config.ini>
; if file is on local filesystem then no syncing will be done
location=config/example.ini

; git repository of project
project = https://github.com/edina/fieldtrip-example

; the java package of the abndroid application
package=uk.ac.edina.mobile

; the install directory
runtime_dir=ftex

; details for config.xml
author_email=example@ed.ac.uk
access_urls=
version=1.0.0.0
url=

; details for mapserver urls
; mapserver_urls=<urls comma separated>
; mapserver_names=<names for the urls above comma separated>

; details for pcapi urls
; pcapi_urls=<urls comma separated>
; pcapi_names=<names for the urls above comma separated>

[release]

; the public release root URL
url=<url for releases>

; accounts and machine names hosting the release
hosts=<hosts comma separated>

; keystore for signing offical releases
keystore=<user>@<domain>:<path for keystore file>

; the filesystem directory holding releases
dir=<path>

; email notification details
smtp=<smtp>
sender=<email>

; notified of beta release
email_beta=<mailist>

; notified of official release
email_official=<email>
